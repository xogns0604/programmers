//https://programmers.co.kr/learn/courses/30/lessons/1835

package LV2.JadenCaseStr;

public class Solution {
	public String solution(String s) {
		String[] strs = s.split(" ");
		
		String answer = "";
		
		for(String str : strs) {
			answer += str.substring(0,1).toUpperCase() + str.substring(1,str.length());
			answer += " ";
		}
		
        return answer;
    }
}

